<p style="color: green"><%= notice %></p>

<h1>Booked Times</h1>

<table>
  <thead>
    <tr>
      <th>Section</th>
      <th>Subject</th>
      <th>Teacher</th>
      <th>Class</th>
      <th>Day</th>
      <th>Start Time</th>
      <th>End Time</th>
    </tr>
  </thead>

  <tbody>
    <% @booked_times.find_each do |booked_time| %>
      <tr>
        <td><%= booked_time.section.name %></td>
        <td><%= booked_time.section.subject.name %></td>
        <td><%= booked_time.section.teacher.full_name %></td>
        <td><%= booked_time.section.class_room.name %></td>
        <td><%= booked_time.day %></td>
        <td><%= formatted_time booked_time.start_time %></td>
        <td><%= formatted_time booked_time.end_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
